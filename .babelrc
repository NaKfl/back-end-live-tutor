{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": 3,
        "targets": "> 0.25%, not dead"
      }
    ],
    ["minify"]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "test": ".test",
          "underscore": "lodash"
        }
      }
    ]
  ],
  "ignore": ["**/__tests__", "**/*.test.js", "**/apidoc"]
}
